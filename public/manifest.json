{
  "manifest_version": 2,
  "name": "e-Clock in",
  "version": "2020.04.14.0",
  "description": "勤怠管理サービス「e-革新」のログインをサポート",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.e-kakushin.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options/index.html"
}
